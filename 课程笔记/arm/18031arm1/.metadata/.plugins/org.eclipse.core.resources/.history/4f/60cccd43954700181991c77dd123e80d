

.text
.globl _start
_start:
	nop

	ldmia sp!,{r0-r5}
	ldmfd sp!,{r0-r5}
	ldmfd sp,{r0-r5}

	ldr r0,=0x40008000
	ldr r1,=0x41000000

	ldm r0,{r1,r6,r3}

loop1:
	ldm r0!,{r2-r11}
	stm r1!,{r2-r11}
	b loop1

	#while(i--);
	mov r5,#40
loop:
	ldr r2,[r0],#4
	str r2,[r1],#4

	subs r5,r5,#1
	bne loop


	ldrsb r2,data




	ldr r0,c
	ldrh r0,c
	ldrb r0,c

	ldr r0,a
	ldr r1,b

	add r0,r0,r1

	str r0,a

_stop:
	b _stop

a:
	.word 10
b:
	.word 20

c:
	.word 0xffffffff
data:
	.byte -1
	.byte -1
	.byte -1
	.byte -1

